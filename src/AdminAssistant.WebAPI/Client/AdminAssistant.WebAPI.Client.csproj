<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Product>Admin Assistant</Product>
    <nullable>enable</nullable>
    <LangVersion>latest</LangVersion>
    <Platforms>x64</Platforms>
    <RootNamespace>AdminAssistant.WebAPI</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <DocumentationFile>AdminAssistant.WebAPI.Client.docs.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <DocumentationFile>AdminAssistant.WebAPI.Client.docs.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="AdminAssistant.WebAPI.Client.docs.xml" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Shared\AdminAssistant.WebAPI.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="v1\" />
  </ItemGroup>

</Project>
