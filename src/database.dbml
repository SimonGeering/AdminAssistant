}

Table "Budget.Budget" 
{
  "BudgetID" INT [pk]
  "AuditID" INT
  "OwnerID" INT
  "BudgetName" NVARCHAR(50)
}
Ref: "Budget.Budget"."BudgetID" < "Budget.BudgetEntry"."BudgetID"

