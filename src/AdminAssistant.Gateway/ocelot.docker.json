{
  "Routes": [

    // Ops - http://localhost:16686/search
    {
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "adminassistant.ops.jaeger",
          "Port": 16686
        }
      ],
      "DownstreamHeaderTransform": {
        "Referrer": "http://example.com, http://localhost:16686",
        "Host": "example.com, localhost:16686",
        "Origin": "http://example.com, http://localhost:16686"
      },
      "Priority": 10,
      "UpstreamPathTemplate": "/jaeger/{url}", // "http://localhost/jaeger/search
      "UpstreamHttpMethod": [ "Get" ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
