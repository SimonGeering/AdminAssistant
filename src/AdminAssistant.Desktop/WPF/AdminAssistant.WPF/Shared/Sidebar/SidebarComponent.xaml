<UserControl x:Class="AdminAssistant.WPF.Shared.Sidebar.SidebarComponent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             DataContext="{Binding SidebarViewModel, Source={StaticResource ViewModelLocator}}">
  <Grid>
    <Label Content="Side Bar" />
    <StackPanel>
      <Button Content="Dashboard" />
      <Button Content="Mail" />
      <Button Content="Calendar" />
      <Button Content="Contacts" />
      <Button Content="Tasks" />
      <Button Content="Accounts" />
      <Button Content="Assets" />
      <Button Content="Billing" />
      <Button Content="Budget" />
      <Button Content="Documents" />
      <Button Content="Reports" />
      <Button Content="Admin" />
    </StackPanel>
  </Grid>
</UserControl>
